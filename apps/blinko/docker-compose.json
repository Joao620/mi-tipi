{
  "services": [
    {
      "name": "blinko-website",
      "image": "blinkospace/blinko@sha256:5f41fe4ff2e4030495b3a18a0a1a1722fe4c6544b2cbd6b4fae741f47c780e8c",
      "pull_policy": "missing",
      "environment": {
        "DATABASE_URL": "postgresql://postgres:${DB_ROOT_PASSWORD}@blinko-postgres:5432/postgres",
        "NODE_ENV": "${NODE_ENV}",
        "NEXTAUTH_URL": "${NEXTAUTH_URL}",
        "NEXT_PUBLIC_BASE_URL": "${NEXT_PUBLIC_BASE_URL}",
        "NEXTAUTH_SECRET": "${NEXTAUTH_SECRET}"
      },
      "internalPort": 1111,
      "isMain": true,
      "healthCheck": {
        "test": "wget -nv -t1 --spider 'http://127.0.0.1:1111/'",
        "interval": "30s",
        "timeout": "10s",
        "retries": 5,
        "start_period": "30s"
      },
      "logging": {
        "driver": "json-file",
        "options": {
          "max-size": "10m",
          "max-file": "3"
        }
      },
      "dependsOn": {
        "blinko-postgres": {
          "condition": "service_healthy"
        }
      }
    },
    {
      "name": "blinko-postgres",
      "image": "postgres@sha256:e8b5c91b9b63b5a7574cbcfa87c3d74167700bffef5bfcb95778c25f0b50e0c4",
      "pull_policy": "missing",
      "environment": {
        "POSTGRES_DB": "postgres",
        "POSTGRES_USER": "postgres",
        "POSTGRES_PASSWORD": "${DB_ROOT_PASSWORD}",
        "TZ": "Asia/Shanghai"
      },
      "internalPort": 5432,
      "healthCheck": {
        "test": "pg_isready -U postgres -d postgres",
        "interval": "5s",
        "timeout": "10s",
        "retries": 5
      }
    }
  ]
}